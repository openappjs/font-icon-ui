{
  "name": "font-icon-ui",
  "version": "0.0.0",
  "description": "generates a font-icon mercury component with a pseudo-element, existing font-face and css",
  "main": "index.js",
  "scripts": {
    "example": "beefy -p 5000 examples/grid/index.js",
    "examples": "exampler-develop",
    "examples-build": "exampler-build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/openappjs/font-icon-ui.git"
  },
  "keywords": [
    "mercury",
    "font",
    "icon"
  ],
  "browserify": {
    "transform": [
      "cssify",
      "envify",
      "brfs"
    ]
  },
  "author": "openappjs",
  "license": "AGPL, PPL",
  "bugs": {
    "url": "https://github.com/openappjs/font-icon-ui/issues"
  },
  "homepage": "https://github.com/openappjs/font-icon-ui",
  "dependencies": {
    "grid-ui": "^1.2.0",
    "mercury": "^6.0.2",
    "rcss": "^0.1.3",
    "to-double-quotes": "^1.0.0",
    "to-single-quotes": "^1.0.1",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "brfs": "^1.2.0",
    "css-face-string": "0.0.2",
    "cssify": "^0.6.0",
    "cssom": "^0.3.0",
    "cssparser": "^0.2.2",
    "envify": "^3.0.0",
    "exampler": "^1.0.0",
    "font-awesome": "^4.2.0",
    "insert-css": "^0.2.0",
    "list-ui": "^1.0.1",
    "lodash": "^2.4.1",
    "ndarray": "^1.0.15"
  }
}
